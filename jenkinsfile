pipeline {
    agent any
    tools{
        maven 'maven-3.9.5'
    }
    stages{
        stage('Build Maven'){
            steps{
                     checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Divya7508/cicd2.git']])                
                     sh 'mvn clean install'
            }
        }
     }
}
